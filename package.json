{
  "name": "package-search",
  "version": "0.0.2",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "nps": "dist/main.js"
  },
  "scripts": {
    "build": "npx webpack --entry ./src/index.jsx --output dist/main.js --mode production",
    "build:dev": "npx webpack --entry ./src/index.jsx --output dist/main.js --mode development",
    "start": "npm run build:dev && ./dist/main.js",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "test": "jest",
    "test:update": "jest --u"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "ink-testing-library": "^1.0.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "github-url-to-object": "^4.0.4",
    "ink": "^2.6.0",
    "ink-text-input": "^3.2.2",
    "ink-use-stdout-dimensions": "^1.0.5",
    "node-fetch": "^2.6.0",
    "npm-registry-fetch": "^8.1.0",
    "react": "^16.13.1"
  }
}
