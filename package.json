{
  "name": "package-search",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "nps": "dist/main.js"
  },
  "scripts": {
    "build": "npx webpack --entry ./src/index.jsx --output dist/main.js --mode production",
    "build:dev": "npx webpack --entry ./src/index.jsx --output dist/main.js --mode development",
    "start": "npm run build:dev && ./dist/main.js",
    "lint": "eslint src/*",
    "lint:fix": "eslint src/* --fix"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "get-package-readme": "^1.3.0",
    "ink": "^2.6.0",
    "ink-text-input": "^3.2.2",
    "ink-use-stdout-dimensions": "^1.0.5",
    "libnpmsearch": "^2.0.2",
    "react": "^16.12.0"
  }
}
