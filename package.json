{
  "name": "package-search",
  "version": "0.0.2",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "nps": "dist/main.js"
  },
  "scripts": {
    "build": "npx webpack --entry ./src/index.jsx --output dist/main.js --mode production",
    "build:dev": "npx webpack --entry ./src/index.jsx --output dist/main.js --mode development",
    "start": "npm run build:dev && ./dist/main.js",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "test": "jest --coverage",
    "test:update": "jest --u"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "ink-testing-library": "^1.0.3",
    "jest": "^25.1.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "get-package-readme": "^1.3.0",
    "ink": "^2.6.0",
    "ink-text-input": "^3.2.2",
    "ink-use-stdout-dimensions": "^1.0.5",
    "libnpmsearch": "^3.0.0",
    "react": "^16.12.0"
  }
}
